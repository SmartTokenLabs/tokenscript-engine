	<ts:contract name="SevenDragonsScore">
		
		
		
		<ts:address network="8217">0x13585Dc6268B0B5424d47aF4f63D8A50D6F56CF8</ts:address>
		<ts:abi>
			
			[
				{
				  "inputs": [
					{
					  "internalType": "uint256",
					  "name": "tokenId",
					  "type": "uint256"
					}
				  ],
				  "name": "getDragonInfo",
				  "outputs": [
					{
					  "components": [
						{
						  "components": [
							{
							  "internalType": "uint8",
							  "name": "level",
							  "type": "uint8"
							},
							{
							  "internalType": "uint16",
							  "name": "numHunts",
							  "type": "uint16"
							},
							{
							  "internalType": "uint256",
							  "name": "lastHunt",
							  "type": "uint256"
							},
							{
							  "internalType": "uint16",
							  "name": "numFlies",
							  "type": "uint16"
							},
							{
							  "internalType": "uint256",
							  "name": "lastFly",
							  "type": "uint256"
							},
							{
							  "internalType": "uint16",
							  "name": "numFires",
							  "type": "uint16"
							},
							{
							  "internalType": "uint256",
							  "name": "lastFire",
							  "type": "uint256"
							},
							{
							  "internalType": "uint256[]",
							  "name": "friends",
							  "type": "uint256[]"
							}
						  ],
						  "internalType": "struct SevenDragonsScore.DragonState",
						  "name": "state",
						  "type": "tuple"
						},
						{
						  "internalType": "uint256",
						  "name": "pointsBalance",
						  "type": "uint256"
						},
						{
						  "internalType": "uint256",
						  "name": "actionLimitReset",
						  "type": "uint256"
						},
						{
						  "internalType": "uint16",
						  "name": "actionLimitCount",
						  "type": "uint16"
						},
						{
						  "internalType": "bool",
						  "name": "canHunt",
						  "type": "bool"
						},
						{
						  "internalType": "bool",
						  "name": "canBreathFire",
						  "type": "bool"
						},
						{
						  "internalType": "bool",
						  "name": "canFly",
						  "type": "bool"
						},
						{
						  "internalType": "bool",
						  "name": "canLevelUp",
						  "type": "bool"
						}
					  ],
					  "internalType": "struct SevenDragonsScore.DragonInfo",
					  "name": "",
					  "type": "tuple"
					}
				  ],
				  "stateMutability": "view",
				  "type": "function"
				}
			]
			
		</ts:abi>
	</ts:contract>

	<ts:origins>
		
		<ts:ethereum contract="SevenDragonsNFT"/>
	</ts:origins>

	<ts:selection filter="canHunt=FALSE" name="alreadyHunted">
		<ts:label>
			<ts:string xml:lang="en">Already fed!</ts:string>
		</ts:label>
	</ts:selection>
	<ts:selection filter="canFly=FALSE" name="alreadyFlew">
		<ts:label>
			<ts:string xml:lang="en">Too tired!</ts:string>
		</ts:label>
	</ts:selection>
	<ts:selection filter="canBreathFire=FALSE" name="alreadyBreathedFire">
		<ts:label>
			<ts:string xml:lang="en">Already cleaned!</ts:string>
		</ts:label>
	</ts:selection>
	<ts:selection filter="canLevelUp=FALSE" name="cantLevelUp">
		<ts:label>
			<ts:string xml:lang="en">Can't level up yet!</ts:string>
		</ts:label>
	</ts:selection>
	<ts:selection filter="level=0" name="notSummonedYet">
		<ts:label>
			<ts:string xml:lang="en">You must summon your dragon first!</ts:string>
		</ts:label>
	</ts:selection>
	<ts:selection filter="level&gt;0" name="summoned">
