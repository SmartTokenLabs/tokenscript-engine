		<ts:card name="Info" type="token">
			<ts:label>
				<ts:string xml:lang="en">Loot Info</ts:string>
			</ts:label>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="info" xml:lang="en">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>

		<ts:card name="Play" type="action">
			<ts:label>
				<ts:string xml:lang="en">Play with Smart Cat</ts:string>
			</ts:label>
			<ts:attribute name="catId">
				<ts:type><ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax></ts:type>
				<ts:origins>
					<ts:user-entry as="uint"/>
				</ts:origins>
			</ts:attribute>
			<ts:transaction>
				<ethereum:transaction as="uint" contract="SmartCatLoot" function="play">
					<ts:data>
						<ts:uint256 ref="tokenId"/>
						<ts:uint256 local-ref="catId"/>
					</ts:data>
				</ethereum:transaction>
			</ts:transaction>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="play" xml:lang="en">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>

		<ts:card exclude="BonusUsed" name="CatKombatBonus" type="action">
			<ts:label>
				<ts:string xml:lang="en">Use Cat Kombat Bonus</ts:string>
			</ts:label>
			<ts:attribute name="catId">
				<ts:type><ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax></ts:type>
				<ts:origins>
					<ts:user-entry as="uint"/>
				</ts:origins>
			</ts:attribute>
			<ts:transaction>
				<ethereum:transaction as="uint" contract="CatKombatLuckyDraw" function="claimLootBonus">
					<ts:data>
						<ts:uint256 ref="tokenId"/>
						
						
						
						
						<ts:address>0xa532D3c016aA0B44b7228aCcd701A5F03112CD22</ts:address>
						<ts:uint256 local-ref="catId"/>
					</ts:data>
				</ethereum:transaction>
			</ts:transaction>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="catKombatBonus" xml:lang="en">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>

		<ts:card name="Burn" origins="SmartCatLoot" type="action">
			<ts:label>
				<ts:string xml:lang="en">Burn</ts:string>
			</ts:label>
			<ts:attribute name="invitee">
				<ts:type><ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax></ts:type>
				<ts:origins>
					<ts:user-entry as="uint"/>
				</ts:origins>
			</ts:attribute>
			<ts:transaction>
				<ethereum:transaction as="uint" contract="SmartCatLoot" function="burn">
					<ts:data>
						<ts:uint256 ref="tokenId"/>
					</ts:data>
				</ethereum:transaction>
			</ts:transaction>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" urlFragment="burn" xml:lang="en">
				<ts:viewContent name="common"/>
			</ts:view>
		</ts:card>

	</ts:cards>

	<ts:attribute name="points">
		<ts:type><ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax></ts:type>
		<ts:label>
			<ts:string xml:lang="en">SmartLayer Points</ts:string>
		</ts:label>
		<ts:origins>
			<ethereum:call as="uint" contract="SmartCatLoot" function="pointsBalance">
				<ts:data>
					<ts:uint256 ref="tokenId"/>
				</ts:data>
			</ethereum:call>
		</ts:origins>
	</ts:attribute>

	<ts:attribute name="maticPayout">
		<ts:type><ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax></ts:type>
		<ts:label>
			<ts:string xml:lang="en">Matic Payout</ts:string>
		</ts:label>
		<ts:origins>
			<ethereum:call as="uint" contract="SmartCatLoot" function="maticPayout">
				<ts:data/>
			</ethereum:call>
		</ts:origins>
	</ts:attribute>

	<ts:attribute name="wethPayout">
		<ts:type><ts:syntax>1.3.6.1.4.1.1466.115.121.1.27</ts:syntax></ts:type>
		<ts:label>
			<ts:string xml:lang="en">wETH Payout</ts:string>
		</ts:label>
		<ts:origins>
			<ethereum:call as="uint" contract="SmartCatLoot" function="wethPayout">
				<ts:data/>
			</ethereum:call>
		</ts:origins>
	</ts:attribute>

	<ts:attribute name="bonusUsed">
		<ts:type><ts:syntax>1.3.6.1.4.1.1466.115.121.1.7</ts:syntax></ts:type>
		<ts:label>
			<ts:string xml:lang="en">CatKombat Bonus Used</ts:string>
		</ts:label>
		<ts:origins>
			<ethereum:call as="bool" contract="CatKombatLuckyDraw" function="isLootBonusUsed">
				<ts:data>
					<ts:uint256 ref="tokenId"/>
				</ts:data>
			</ethereum:call>
		</ts:origins>
	</ts:attribute>

